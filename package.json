  },
  "exports": {
    "require": ".dist/index.cjs",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "devDependencies": {
    "@eslint/js": "9.11.1",
    "@rollup/plugin-swc": "0.4.0",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.26",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "globals": "15.9.0",
    "happy-css-modules": "3.1.1",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "rollup": "4.22.4",
    "tslib": "2.7.0",
    "typescript": "5.6.2",
    "typescript-eslint": "8.7.0",
    "vite": "5.4.7"
  },
  "peerDependencies": {
    "happy-css-modules": ">= 3",
    "vite": ">= 5"
  },
  "volta": {
    "node": "20.17.0",
    "npm": "10.8.3"
