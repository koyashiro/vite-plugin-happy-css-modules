  },
  "license": "MIT",
  "author": "koyashiro",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koyashiro/vite-plugin-happy-css-modules.git"
  },
  "scripts": {
    "build": "rm -rf ./dist/ && tsc && rollup -c",
