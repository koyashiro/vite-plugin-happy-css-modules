    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.5.0",
    "happy-css-modules": "4.0.0",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "4.3.0",
    "rollup": "4.54.0",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1",
    "vite": "7.3.0"
  },
  "peerDependencies": {
    "happy-css-modules": ">= 4",
    "vite": ">= 7"
